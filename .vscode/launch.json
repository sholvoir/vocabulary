    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "build",
            "type": "node",
            "program": "${workspaceFolder}/build.ts",
            "args": ["cet6-yd2"],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/home/shaw/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "spell",
            "type": "node",
            "program": "${workspaceFolder}/lib/spell-check.ts",
            "args": ["depend on", "1"],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/home/shaw/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "comp",
            "type": "node",
            "program": "${workspaceFolder}/compare.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/usr/bin/deno",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        }
    ]
}